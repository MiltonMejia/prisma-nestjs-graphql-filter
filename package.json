{
	"name": "prisma-nestjs-graphql-filter",
	"version": "1.3.1",
	"description": "This package helps you to reduce boilerplate if you want to create Prisma filter input in Graphql",
	"main": "lib/index.js",
	"types": "lib/index.d.ts",
	"scripts": {
		"test": "jest --config jestconfig.json",
		"build": "rimraf dist && tsc",
		"prepublish": "npm run build",
		"format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
		"lint": "tslint -p tsconfig.json"
	},
	"keywords": [
		"graphql",
		"nestjs",
		"decorator",
		"prisma",
		"input",
		"orderBy"
	],
	"author": "mefam",
	"license": "MIT",
	"devDependencies": {
		"@nestjs/graphql": "^10.2.0",
		"@nestjs/testing": "^9.3.9",
		"@types/jest": "^29.4.0",
		"@typescript-eslint/eslint-plugin": "^5.52.0",
		"@typescript-eslint/parser": "^5.52.0",
		"class-transformer": "^0.5.1",
		"eslint": "^8.34.0",
		"eslint-config-xo": "^0.43.1",
		"eslint-config-xo-typescript": "^0.56.0",
		"graphql": "^16.6.0",
		"jest": "^29.4.3",
		"prettier": "^2.8.4",
		"rimraf": "^4.1.2",
		"ts-jest": "^29.0.5",
		"ts-morph": "^17.0.1",
		"typescript": "^4.9.5"
	},
	"files": [
		"lib/**/*"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/MiltonMejia/prisma-nestjs-graphql-filter.git"
	},
	"dependencies": {
		"prisma-graphql-type-decimal": "^2.0.6"
	}
}
