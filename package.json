{
	"name": "prisma-nestjs-graphql-filter",
	"version": "1.3.3",
	"description": "This package helps you to reduce boilerplate if you want to create Prisma filter input in Graphql",
	"main": "lib/index.js",
	"types": "lib/index.d.ts",
	"scripts": {
		"test": "jest --config jestconfig.json",
		"build": "rimraf dist && tsc",
		"prepublish": "npm run build",
		"format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
		"lint": "tslint -p tsconfig.json"
	},
	"keywords": [
		"graphql",
		"nestjs",
		"decorator",
		"prisma",
		"input",
		"orderBy"
	],
	"author": "mefam",
	"license": "MIT",
	"devDependencies": {
		"@nestjs/graphql": "^12.0.8",
		"@nestjs/testing": "^10.0.5",
		"@types/jest": "^29.5.3",
		"@typescript-eslint/eslint-plugin": "^6.0.0",
		"@typescript-eslint/parser": "^6.0.0",
		"class-transformer": "^0.5.1",
		"eslint": "^8.44.0",
		"eslint-config-xo": "^0.43.1",
		"eslint-config-xo-typescript": "^0.57.0",
		"graphql": "^16.7.1",
		"jest": "^29.6.1",
		"prettier": "^3.0.0",
		"rimraf": "^5.0.1",
		"ts-jest": "^29.1.1",
		"ts-morph": "^19.0.0",
		"typescript": "^5.1.6"
	},
	"files": [
		"lib/**/*"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/MiltonMejia/prisma-nestjs-graphql-filter.git"
	}
}
